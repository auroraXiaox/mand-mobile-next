.md-popup-title-bar {
  --md-popup-title-bar-bg: var(--md-color-bg-inverse);
  --md-popup-title-bar-height: 120px;
  --md-popup-title-bar-height-large: 180px;
  --md-popup-title-bar-radius: 28px;
  --md-popup-title-bar-radius-large: 40px;
  --md-popup-title-bar-font-size-button: var(--md-font-caption-large);
  --md-popup-title-bar-font-weight-button: var(--md-font-weight-medium);
  --md-popup-title-bar-font-size-title: 40px;
  --md-popup-title-bar-font-size-describe: var(--md-font-body-large);
  --md-popup-title-bar-color-title: var(--md-color-text-base);
  --md-popup-title-bar-color-describe: var(--md-color-text-caption);
  --md-popup-title-bar-color-button-left: var(--md-color-text-minor);
  --md-popup-title-bar-color-button-right: var(--md-color-text-highlight);
}
.md-popup-title-bar {
  position: relative;
  width: 100%;
  background-color: var(--md-popup-title-bar-bg);
  border-radius: var(--md-popup-title-bar-radius) var(--md-popup-title-bar-radius) 0 0;
  overflow: hidden;
}
.md-popup-title-bar.large {
  height: var(--md-popup-title-bar-height-large);
}
.md-popup-title-bar.title-align-left .title-bar-title {
  padding-left: var(--md-h-gap-sl);
  align-items: flex-start;
}
.md-popup-title-bar.title-align-left .title-bar-left {
  display: none;
}
.md-popup-title-bar.title-align-right .title-bar-title {
  padding-right: var(--md-h-gap-sl);
  align-items: flex-end;
}
.md-popup-title-bar.title-align-right .title-bar-right {
  display: none;
}
.md-popup-title-bar > div {
  display: flex;
  padding-top: 60px;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  text-overflow: ellipsis;
  word-break: break-all;
  word-wrap: break-word;
  white-space: nowrap;
}
.md-popup-title-bar .title-bar-left,
.md-popup-title-bar .title-bar-right {
  position: absolute;
  top: 0;
  width: 20%;
  max-height: var(--md-popup-title-bar-height);
  font-size: var(--md-popup-title-bar-font-size-button);
  font-weight: var(--md-popup-title-bar-font-weight-button);
  box-sizing: border-box;
  line-height: 1;
}
.md-popup-title-bar .title-bar-title {
  width: 100%;
  padding-left: 20%;
  padding-right: 20%;
  box-sizing: border-box;
  line-height: 1;
}
.md-popup-title-bar .title-bar-title p.title {
  height: 50px;
  font-size: var(--md-popup-title-bar-font-size-title);
  color: var(--md-popup-title-bar-color-title);
}
.md-popup-title-bar .title-bar-title p.describe {
  margin-top: 15px;
  font-size: var(--md-popup-title-bar-font-size-describe);
  color: var(--md-popup-title-bar-color-describe);
}
.md-popup-title-bar .title-bar-left {
  left: 0;
  padding-left: var(--md-h-gap-sl);
  align-items: flex-start;
}
.md-popup-title-bar .title-bar-right {
  right: 0;
  padding-right: var(--md-h-gap-sl);
  align-items: flex-end;
}
.md-popup-title-bar .md-popup-cancel {
  color: var(--md-popup-title-bar-color-button-left);
}
.md-popup-title-bar .md-popup-confirm {
  color: var(--md-popup-title-bar-color-button-right);
}
.md-popup-title-bar .md-popup-close {
  padding-top: var(--md-h-gap-sl);
  color: var(--md-popup-title-bar-color-button-left);
  justify-content: flex-start;
}
