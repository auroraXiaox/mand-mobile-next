.md-picker,
.md-picker-view,
.inner-popup {
  --md-picker-padding-h: var(--md-h-gap-sl);
  --md-picker-font-size: var(--md-font-caption-normal);
  --md-picker-disabled-opacity: 0.2;
  --md-picker-color: var(--md-color-text-disabled);
  --md-picker-color-active: var(--md-color-text-highlight);
  --md-picker-font-weight-active: var(--md-font-weight-medium);
  --md-picker-border-color: var(--md-color-border-base);
  --md-picker-zindex: 1100;
}
.md-picker-view {
  position: relative;
  width: 100%;
  padding: 0 var(--md-picker-padding-h);
  background: var(--md-color-bg-inverse);
  box-sizing: border-box;
  transform: translate3d(0, 0, 0);
}
.md-picker-view .md-picker-view_container {
  position: relative;
  height: 100%;
  overflow: hidden;
}
.md-picker-view .md-picker-view_container .md-picker-view_masker {
  position: absolute;
  height: 100px;
  left: 0;
  right: 0;
}
.md-picker-view .md-picker-view_container .md-picker-view_masker:before,
.md-picker-view .md-picker-view_container .md-picker-view_masker:after {
  content: "";
  width: 100%;
  display: block;
  position: absolute;
  transform-origin: 0 0;
}
.md-picker-view .md-picker-view_container .md-picker-view_masker--top {
  top: 0;
}
.md-picker-view .md-picker-view_container .md-picker-view_masker--top:after {
  bottom: 0;
  border-bottom: solid 1px var(--md-picker-border-color);
  transform-origin: 0 bottom;
}
.md-picker-view .md-picker-view_container .md-picker-view_masker--bottom {
  bottom: 0;
}
.md-picker-view .md-picker-view_container .md-picker-view_masker--bottom:before {
  top: 0;
  border-top: solid 1px var(--md-picker-border-color);
  transform-origin: 0 top;
}
.md-picker-view .md-picker-view_container .md-picker-view_wheel_wrapper {
  display: flex;
  z-index: 10;
}
.md-picker-view .md-picker-view_container .md-picker-view_wheel_column {
  position: relative;
  flex: 1;
  overflow: hidden;
}
.md-picker-view .md-picker-view_container .md-picker-view_wheel {
  margin-top: 100px;
}
.md-picker-view .md-picker-view_container .md-picker-view_wheel--item {
  width: 100%;
  padding: 0 var(--md-h-gap-sm);
  box-sizing: border-box;
  color: var(--md-picker-color);
  font-size: var(--md-picker-font-size);
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.md-picker-view .md-picker-view_container .md-picker-view_wheel--item.active {
  color: var(--md-picker-color-active);
  font-weight: var(--md-picker-font-weight-active);
}
.md-picker-view .md-picker-view_container .md-picker-view_wheel--disabled-item {
  opacity: var(--md-picker-disabled-opacity);
}
.md-picker {
  width: 100%;
}
.md-picker .md-popup {
  z-index: 1100;
}
