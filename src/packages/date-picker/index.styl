}
.md-popup-mask {
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  position: absolute;
  pointer-events: auto;
  z-index: 1;
  background-color: var(--md-popup-mask-bg);
}
.md-popup-box {
  position: relative;
  pointer-events: auto;
  z-index: 2;
  max-width: 100%;
  max-height: 100%;
  overflow: auto;
}
.md-popup-mask {
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  position: absolute;
  pointer-events: auto;
  z-index: 1;
  background-color: var(--md-popup-mask-bg);
}
.md-mask-fade-enter-from,
.md-mask-fade-leave-to {
  opacity: 0.01;
}
.md-mask-fade-enter-active,
.md-mask-fade-leave-active {
  transition: opacity 0.25s;
}
